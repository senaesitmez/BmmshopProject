<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="~/theme/css/style.css">
    <link rel="stylesheet" href="https://fontawesome.com/v4/assets/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/theme/css/stylenew.css">
</head>
<body>

    <nav id="navbar" class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">

            <img src="~/theme/img/analogo.png" alt="..." class="navbar-brand" height="60" width="150">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>


            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active px-3">
                        <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                    </li>
                </ul>
                <form class="form-inline mx-lg-auto">
                    <input class="form-control" type="search" placeholder="Search the product" aria-label="Search">
                    <button class="btn btn-outline-info my-2 my-sm-0" type="submit"><i class="bi bi-search"></i></button>
                    <ul class="navbar-nav mr-auto">
                        <li class="dropdown">
                            <button class="btn btn-dark dropdown-toggle mr-2 ml-4" type="button" id="loginButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-box-arrow-in-right mr-2"></i>
                                Sign In
                            </button>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                <a class="dropdown-item" href="/Home/Signin">Sign In</a>
                                <a class="dropdown-item" href="/Home/Signup">Sign Up</a>
                            </div>
                        </li>
                        <li class="nav-item active">
                            <a class="nav-link" href="#"><i class="bi bi-basket-fill" role="img" aria-label="GitHub"></i> Basket <span class="sr-only">(current)</span></a>
                        </li>
                    </ul>
                </form>
            </div>
        </div>
    </nav>
    <nav class="navbar navbar-dark bg-light mb-2">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="dropdown">
                        <div class="btn-group">
                            <a href="#electronicSection" class="btn btn-light mr-3 ml-2" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-phone"></i>
                                Electronic
                            </a>
                        </div>
                        <div class="btn-group">
                            <a href="#fashionSection" class="btn btn-light mr-3" id="fashionDropDown" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-bag"></i>
                                Fashion
                            </a>
                        </div>
                        <div class="btn-group">
                            <a href="#homeSection" class="btn btn-light  mr-3" id="homeDropDown" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-house-door"></i>
                                Home,Life
                            </a>
                        </div>
                        <div class="btn-group">
                            <a href="#sportSection" class="btn btn-light  mr-3" id="sportDropDown" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-activity"></i>
                                Sport,Outdoor
                            </a>
                        </div>
                        <div class="btn-group">
                            <a href="#cosmeticSection" class="btn btn-light  mr-3" id="cosmeticDropDown" ria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-person"></i>
                                Cosmetics,Personal Care
                            </a>
                        </div>
                        <div class="btn-group">
                            <a href="#bookSection" class="btn btn-light  mr-3" id="bookDropDown" aria-haspopup="true" aria-expanded="false">
                                <i class="bi bi-book"></i>
                                Book,Music,Movie
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>
    <footer class="footer">
        <div class="container">
            <span class="text-muted">
                Place sticky footer content here.
                <a href="#navbar" class="btn btn-light mr-3 ml-2" aria-haspopup="true" aria-expanded="false">
                    <i class="bi bi-arrow-up"></i>
                    Back to top
                </a>
            </span>
        </div>
    </footer>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="/theme/js/holder.min.js" type="text/javascript"></script>
    <script src="~/theme/js/script.js" type="text/javascript"></script>
</body>
</html>